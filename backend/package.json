{
  "name": "dronuts-2023-swac",
  "description": "Dronuts startup idea for 17-356 Spring 2023",
  "author": "SWAC Team (Steven, Wei-Chang, Ashwin, Claire)",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://backendtest1.fly.dev/",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "concurrently": "^8.0.1",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.32.1",
    "eslint-watch": "^8.0.0",
    "jest": "^29.4.3",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "axios": "^1.3.3",
    "chai-datetime": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "expect.js": "^0.3.1",
    "express": "^4.18.2",
    "mongoose": "^6.9.2",
    "npm-check": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "sinon": "^15.0.1",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "jest": {
    "testTimeout": 1500,
    "express": "^4.18.2",
    "npm-check": "^6.0.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "test": "jest",
    "start-backend": "ts-node --experimental-specifier-resolution=node ./server.ts",
    "start-backend-alt": "node --experimental-specifier-resolution=node --loader ts-node/esm ./server.ts",
    "start": "node server.js",
    "client": "npm start --prefix ../frontend",
    "dev": "concurrently \"npm run start-backend\" \"npm run client\""
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es2021": true,
      "jest/globals": true
    },
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-undef": "off",
      "no-unused-vars": "off"
    }
  },
  "type": "module",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
